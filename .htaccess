# This is a customized WP htaccess to use with WP Imager - Keep it outside of the begin/end WP rules, or it will get overwritten
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !^/(cache_img) [NC]
RewriteRule . /index.php [L]
RewriteRule ^cache_img/r/(.*)x(.*)-(.*)/i/(.*) cache_img/tt.php?src=http://%{HTTP_HOST}/$4&w=$1&h=$2&zc=$3&q=100 [L]
</IfModule>

# BEGIN WordPress

# END WordPress